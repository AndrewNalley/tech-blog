
<div class="text-center">
  <h2>{{blog.name}}</h2>
  <p>{{{get_emoji}}}</p>
  <p>{{blog.text}}</p>
  <p>Created by {{blog.user.name}} on {{format_date blog.date_created}}</p>
  <p>
    </div>
    <div class="text-center">
    <button class="btn btn-primary">Comment</button>
  </p>
  {{#each comments as |comment| }}
<p>{{comment.text}}</p>
  {{/each}}
</div>
